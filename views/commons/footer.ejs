<input type="hidden" id="page-info-str" value="<%= dataInfoStr %>">
<script>
    (function () {
        var dataInfoStr = document.querySelector('#page-info-str').value;
        var dataInfo = {};
        try {
            dataInfo = JSON.parse(dataInfoStr);
        } catch (e) {
            dataInfo = dataInfoStr;
        }
        window.dataInfo = dataInfo;
    })();
</script>
